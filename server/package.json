{
  "name": "term-project-api",
  "version": "0.0.1",
  "description": "TypeScript Express MySQL TypeORM REST API",
  "main": "build/server.js",
  "scripts": {
    "dev": "nodemon --transpile-only src/server.ts",
    "build": "tsc",
    "start": "pm2 start ecosystem.config.json",
    "prod": "sudo pm2 start ecosystem.config.json",
    "typeorm": "node --require ts-node/register node_modules/typeorm/cli.js --config src/configs/orm.config.ts",
    "entity:create": "npm run typeorm entity:create -- -n",
    "subscriber:create": "npm run typeorm subscriber:create -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "keywords": [
    "TypeScript",
    "Express",
    "MySQL",
    "TypeORM",
    "REST API"
  ],
  "author": "jeongwoo",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.10",
    "@types/node": "^16.10.3",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.13",
    "prettier": "2.4.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1005.0",
    "axios": "^0.22.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fuse.js": "^6.4.6",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "mysql2": "^2.3.0",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
